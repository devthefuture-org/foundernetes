{
  "name": "@foundernetes/orgz",
  "version": "1.0.0",
  "license": "MIT",
  "description": "ðŸš€ Setup your multi-tenants on Kubernetes with Forgejo and ArgoCD",
  "dependencies": {
    "@foundernetes/async": "workspace:^",
    "@foundernetes/axios": "workspace:^",
    "@foundernetes/blueprint": "workspace:^",
    "@foundernetes/ctx": "workspace:^",
    "@foundernetes/eta": "workspace:^",
    "@foundernetes/execa": "workspace:^",
    "@foundernetes/match": "workspace:^",
    "@foundernetes/std": "workspace:^",
    "fs-extra": "^11.1.1",
    "link-module-alias": "^1.2.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "pkg": "^5.8.0"
  },
  "engines": {
    "node": ">=20"
  },
  "bin": "./orgz",
  "scripts": {
    "build": "yarn build:clean && yarn build:ncc && yarn build:pkg",
    "build:clean": "rm -rf ./dist",
    "build:ncc": "cd ../../ && ncc build modules/orgz/orgz --out modules/orgz/dist --source-map --v8-cache",
    "build:pkg": "cd ../../ && pkg -c modules/orgz/.pkg.json -o modules/orgz/dist-bin/orgz --compress=GZip modules/orgz/dist/index.js",
    "lint": "eslint .",
    "postinstall": "link-module-alias && yarn build:prepare"
  },
  "type": "commonjs",
  "_moduleAliases": {
    "~": "."
  }
}
