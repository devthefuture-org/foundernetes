{
  "name": "@foundernetes/sniper",
  "version": "1.4.2",
  "license": "MIT",
  "description": "ðŸ”« Run arbitraty js scripts with @foundernetes/blueprint framework and embedded yarn autoinstaller",
  "dependencies": {
    "@foundernetes/async": "^1.4.1",
    "@foundernetes/axios": "^1.4.1",
    "@foundernetes/blueprint": "^1.4.1",
    "@foundernetes/ctx": "^1.4.1",
    "@foundernetes/eta": "^1.4.1",
    "@foundernetes/execa": "^1.4.1",
    "@foundernetes/linux": "^1.4.1",
    "@foundernetes/match": "^1.4.1",
    "@foundernetes/std": "^1.4.1",
    "@yarnpkg/cli": "^4.0.1",
    "@yarnpkg/core": "^4.0.1",
    "@yarnpkg/plugin-essentials": "^4.0.1",
    "axios": "^1.4.0",
    "fs-extra": "^11.1.1",
    "link-module-alias": "^1.2.0",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "pkg": "^5.8.0"
  },
  "engines": {
    "node": ">=20"
  },
  "bin": "./sniper",
  "scripts": {
    "build": "yarn build:clean && yarn build:prepare && yarn build:ncc && yarn build:pkg",
    "build:clean": "rm -rf ./dist",
    "build:prepare": "./build-prepare",
    "build:ncc": "cd ../../ && ncc build modules/sniper/sniper --out modules/sniper/dist --source-map --v8-cache",
    "build:pkg": "cd ../../ && pkg -c modules/sniper/.pkg.json -o modules/sniper/dist-bin/sniper --compress=GZip modules/sniper/dist/index.js",
    "lint": "eslint .",
    "postinstall": "link-module-alias && yarn build:prepare"
  },
  "type": "commonjs",
  "_moduleAliases": {
    "~": "."
  },
  "publishConfig": {
    "access": "public"
  }
}
