{
  "name": "@foundernetes/metal-debian",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@foundernetes/async": "workspace:^",
    "@foundernetes/axios": "workspace:^",
    "@foundernetes/blueprint": "workspace:^",
    "@foundernetes/ctx": "workspace:^",
    "@foundernetes/eta": "workspace:^",
    "@foundernetes/execa": "workspace:^",
    "@foundernetes/linux": "workspace:^",
    "@foundernetes/machines": "workspace:^",
    "@foundernetes/match": "workspace:^",
    "@foundernetes/std": "workspace:^",
    "async-retry": "^1.3.3",
    "commander": "^9.4.1",
    "dayjs": "^1.11.7",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.3",
    "enquirer": "^2.3.6",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.0",
    "ini": "^4.1.0",
    "jjo": "^1.2.1",
    "js-yaml": "^4.1.0",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.5.0",
    "micromatch": "^4.0.5",
    "node-random-name": "^1.0.1",
    "node-ssh": "^13.0.1",
    "parse-github-url": "^1.0.2",
    "parse-url": "^8.1.0",
    "pretty-ms": "^8.0.0",
    "qs": "^6.11.0",
    "shell-quote": "^1.8.1",
    "slugify": "^1.6.5",
    "sonic-boom": "^3.2.0",
    "tiged": "^2.12.3",
    "which": "^3.0.0",
    "wildstring": "^1.0.9",
    "ya-retry": "^1.2.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "docsify-cli": "^4.4.4",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-specific-snapshot": "^7.0.0",
    "lint-staged": "^13.0.4",
    "pkg": "^5.8.0",
    "prettier": "^2.8.0",
    "replace": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "start": "yarn playbook",
    "build": "yarn build:clean && yarn build:ncc && yarn build:pkg",
    "build:clean": "rm -rf ./dist",
    "build:ncc": "cd ../../ && ncc build modules/metal-debian/metal-debian --out modules/metal-debian/dist --source-map --v8-cache",
    "build:pkg": "cd ../../ && pkg -c modules/metal-debian/.pkg.json -o modules/metal-debian/dist-bin/metal-debian --compress=GZip modules/metal-debian/dist/index.js",
    "playbook": "./metal-debian playbook",
    "build:foundernetes": "pkg node_modules/.bin/foundernetes",
    "playbook:foundernetes": "foundernetes playbook",
    "lint": "eslint .",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest tests",
    "test:debug": "DEBUG=1 yarn test",
    "test:update-snapshots": "yarn test -u",
    "postinstall": "[ -d '.husky' ] && husky install || true && link-module-alias",
    "precommit": "lint-staged"
  },
  "type": "commonjs",
  "_moduleAliases": {
    "~": "."
  },
  "packageManager": "yarn@3.3.0"
}
