  "license": "MIT",
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "ajv": "^8.11.2",
    "async": "^3.2.4",
    "async-retry": "^1.3.3",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "compare-versions": "^5.0.1",
    "dayjs": "^1.11.7",
    "decompress": "^4.2.1",
    "direct-logger": "^2.13.0",
    "dotenv": "^16.0.3",
    "enquirer": "^2.3.6",
    "execa": "^7.1.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.0",
    "humanize-duration": "^3.27.3",
    "js-yaml": "^4.1.0",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaults": "^4.2.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "micromatch": "^4.0.5",
    "ms": "^2.1.3",
    "nctx": "^1.6.2",
    "object-hash": "^3.0.0",
    "parse-duration": "^1.0.2",
    "parse-github-url": "^1.0.2",
    "pretty-ms": "^8.0.0",
    "qs": "^6.11.0",
    "shell-quote": "^1.8.0",
    "slugify": "^1.6.5",
    "sonic-boom": "^3.2.0",
    "through2": "^4.0.2",
    "tiged": "^2.12.3",
    "untildify": "^4.0.0",
    "which": "^3.0.0",
    "wildstring": "^1.0.9",
    "ya-retry": "^1.2.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "commit-and-tag-version": "^10.1.0",
    "docsify-cli": "^4.4.4",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-specific-snapshot": "^7.0.0",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "replace": "^1.2.2",
    "rollup": "^3.20.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest tests",
    "test:debug": "DEBUG=1 yarn test",
    "test:update-snapshots": "yarn test -u",
    "postinstall": "[ -d '.husky' ] && husky install || true && yarn compile:cjs && link-module-alias",
    "compile:cjs": "yarn compile:cjs:execa",
    "compile:cjs:execa": "EXECA_PATH=\"$(node -e 'process.stdout.write(require.resolve(`execa`))')\" && mkdir -p .cjs/execa && ncc build \"$EXECA_PATH\" -e cross-spawn -e merge-stream -e signal-exit -o .cjs/execa/ncc-dist && rollup .cjs/execa/ncc-dist/index.js --file .cjs/execa/index.js --format cjs",
    "precommit": "lint-staged",
    "release": "commit-and-tag-version",
    "push": "git push -f --follow-tags origin master",
    "delivery": "git push && yarn release && yarn push",
    "dev:docs": "docsify serve docs",
    "docker:foundernetes:build": "DOCKER_BUILDKIT=1 docker build -f Dockerfile -t foundernetes .",
    "docker:foundernetes:run": "docker run -it -v $PWD:/workspace foundernetes",
    "docker:foundernetes": "yarn docker:foundernetes:build && yarn docker:foundernetes:run"
  },
  "_moduleAliases": {
    "~": "src",
    "~cjs": ".cjs"
  },
  "bin": "./bin/foundernetes",
  "files": [
    "bin",
    "src",
    "plugins",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
