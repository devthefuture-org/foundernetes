{
  "name": "foundernetes",
  "version": "1.0.0",
  "description": "Infra Idempotence As A Framework",
  "main": "src/index.js",
  "repository": "https://github.com/devthejo/foundernetes.git",
  "homepage": "https://devthejo.github.io/foundernetes/",
  "author": "devthejo",
  "license": "MIT",
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "async": "^3.2.4",
    "async-retry": "^1.3.3",
    "axios": "^1.2.0",
    "axios-retry": "^3.3.1",
    "commander": "^9.4.1",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.3",
    "enquirer": "^2.3.6",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.5",
    "nctx": "^1.3.0",
    "parse-github-url": "^1.0.2",
    "pino": "^8.7.0",
    "pino-bfuscate": "^1.0.0",
    "pino-pretty": "^9.1.1",
    "pretty-ms": "^8.0.0",
    "qs": "^6.11.0",
    "slugify": "^1.6.5",
    "sonic-boom": "^3.2.0",
    "tiged": "^2.12.3",
    "which": "^3.0.0",
    "zx": "^7.1.1"
  },
  "devDependencies": {
    "docsify-cli": "^4.4.4",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-specific-snapshot": "^7.0.0",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "replace": "^1.2.2",
    "standard-version": "^9.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "standard-version": {
    "scripts": {
      "postcommit": "yarn version-e2e:commit",
      "posttag": "yarn version-major-branch:up && yarn version-major-branch:push"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest tests",
    "test:debug": "DEBUG=1 yarn test",
    "test:update-snapshots": "yarn test -u",
    "postinstall": "[ -d '.husky' ] && husky install || true && link-module-alias",
    "precommit": "lint-staged",
    "release": "standard-version",
    "push": "git push -f --follow-tags origin master",
    "delivery": "git push && yarn release && yarn push",
    "version-major-branch:up": "scripts/dev-tools/bin/version-major-branch",
    "version-major-branch:push": "scripts/dev-tools/bin/version-major-branch-push",
    "version-e2e:commit": "scripts/dev-tools/bin/version-e2e-commit",
    "version-e2e:set": "node scripts/dev-tools/bin/version-e2e-set",
    "version-e2e": "yarn version-e2e:set",
    "dev:docs": "docsify serve docs",
    "docker:foundernetes:build": "DOCKER_BUILDKIT=1 docker build -f Dockerfile -t foundernetes .",
    "docker:foundernetes:run": "docker run -it -v $PWD:/workspace foundernetes",
    "docker:foundernetes": "yarn docker:foundernetes:build && yarn docker:foundernetes:run"
  },
  "_moduleAliases": {
    "~": "src"
  },
  "bin": "./bin/foundernetes",
  "files": [
    "bin",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
